# Interface signals for covers with TX as framelik record
proc XGMII_IBUF_TOP_IFC { IBUF_PATH } {
   add wave -divider "XGMII IBUF interface"
   add wave -divider "Status"
   add_wave "-noupdate -hex -label reg_ibuf_en          " $IBUF_PATH/IBUF_XGMII_SDR_NORECi/MI_INTi/reg_ibuf_en
   
   add wave -divider "XGMII"
   add_wave "-noupdate -hex -label CLK_INT              " $IBUF_PATH/CLK_INT
   add_wave "-noupdate -hex -label XGMII_Receive_Data   " $IBUF_PATH/XGMII_SDR_RXD
   add_wave "-noupdate -bin -label XGMII_Receive_Control" $IBUF_PATH/XGMII_SDR_RXC

   add wave -divider "Sampling unit"
   add_wave "-noupdate -hex -label SAU_REQ          " $IBUF_PATH/SAU_REQ
   add_wave "-noupdate -hex -label SAU_ACCEPT       " $IBUF_PATH/SAU_ACCEPT
   add_wave "-noupdate -hex -label SAU_DV           " $IBUF_PATH/SAU_DV

   add wave -divider "Control data generator"
   add wave -group {CTRL}
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_CLK         " $IBUF_PATH/CTRL_CLK
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_DATA        " $IBUF_PATH/CTRL_DATA
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_REM         " $IBUF_PATH/CTRL_REM
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_SRC_RDY_N   " $IBUF_PATH/CTRL_SRC_RDY_N
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_SOP_N       " $IBUF_PATH/CTRL_SOP_N
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_EOP_N       " $IBUF_PATH/CTRL_EOP_N
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_DST_RDY_N   " $IBUF_PATH/CTRL_DST_RDY_N
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_REQ         " $IBUF_PATH/CTRL_REQ
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_SEND        " $IBUF_PATH/CTRL_SEND
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_RELEASE     " $IBUF_PATH/CTRL_RELEASE
   add_wave "-group {CTRL} -noupdate -hex -label CTRL_RDY         " $IBUF_PATH/CTRL_RDY

   add wave -group {STAT}
   add_wave "-group {STAT} -noupdate -dec -label STAT             " $IBUF_PATH/STAT
   add_wave "-group {STAT} -noupdate -hex -label STAT_VLD         " $IBUF_PATH/STAT_VLD

   add wave -group {FL_TX}
   add_wave "-group {FL_TX} -noupdate -hex -label FL_CLK           " $IBUF_PATH/FL_CLK
   add_wave "-group {FL_TX} -noupdate -hex -label TX_SOF_N    " $IBUF_PATH/TX_SOF_N
   add_wave "-group {FL_TX} -noupdate -hex -label TX_SOP_N    " $IBUF_PATH/TX_SOP_N
   add_wave "-group {FL_TX} -noupdate -hex -label TX_EOP_N    " $IBUF_PATH/TX_EOP_N   
   add_wave "-group {FL_TX} -noupdate -hex -label TX_EOF_N    " $IBUF_PATH/TX_EOF_N
   add_wave "-group {FL_TX} -noupdate -hex -label TX_SRC_RDY_N" $IBUF_PATH/TX_SRC_RDY_N
   add_wave "-group {FL_TX} -noupdate -hex -label TX_DST_RDY_N" $IBUF_PATH/TX_DST_RDY_N
   add_wave "-group {FL_TX} -noupdate -hex -label TX_DATA     " $IBUF_PATH/TX_DATA
   add_wave "-group {FL_TX} -noupdate -hex -label TX_REM      " $IBUF_PATH/TX_REM

   add wave -group {MI}
   add_wave "-group {MI} -noupdate -hex -label MI_CLK     " $IBUF_PATH/MI_CLK
   add_wave "-group {MI} -hex  -label MI_DWR  " $IBUF_PATH/MI_DWR 
   add_wave "-group {MI} -hex  -label MI_ADDR " $IBUF_PATH/MI_ADDR
   add_wave "-group {MI} -hex  -label MI_RD   " $IBUF_PATH/MI_RD  
   add_wave "-group {MI} -hex  -label MI_WR   " $IBUF_PATH/MI_WR  
   add_wave "-group {MI} -hex  -label MI_BE   " $IBUF_PATH/MI_BE  
   add_wave "-group {MI} -hex  -label MI_DRD  " $IBUF_PATH/MI_DRD 
   add_wave "-group {MI} -hex  -label MI_ARDY " $IBUF_PATH/MI_ARDY
   add_wave "-group {MI} -hex  -label MI_DRDY " $IBUF_PATH/MI_DRDY

   add wave -divider "IBUF status signals"
   add_wave "-noupdate -hex -label LINK_UP          " $IBUF_PATH/LINK_UP
   add_wave "-noupdate -hex -label INCOMING_PACKET  " $IBUF_PATH/INCOMING_PCKT
}


proc XGMII_IBUF_IFC { COMP } {
   add wave -divider "XGMII ibuf"
   add_wave "-noupdate -hex -label RESET" $COMP/RESET

   add wave -divider "XGMII interface"
   add_wave "-noupdate -hex -label XGMII_Receive_Clock" $COMP/XGMII_RXCLK
   add_wave "-noupdate -hex -label XGMII_Receive_Data" $COMP/XGMII_RXD
   add_wave "-noupdate -bin -label XGMII_Receive_Control" $COMP/XGMII_RXC

   add wave -divider "Sampling unit interface"
   add_wave "-noupdate -hex -label INT_CLK" $COMP/INT_CLK
   add_wave "-noupdate -hex -label SAU_REQ" $COMP/SAU_REQ
   add_wave "-noupdate -hex -label SAU_ACCEPT" $COMP/SAU_ACCEPT
   add_wave "-noupdate -hex -label SAU_DV" $COMP/SAU_DV

   add wave -divider "Control data generator interface"
   add_wave "-noupdate -hex -label CTRL_CLK" $COMP/CTRL_CLK
   add_wave "-noupdate -hex -label CTRL_DATA" $COMP/CTRL_DATA
   add_wave "-noupdate -hex -label CTRL_REM" $COMP/CTRL_REM
   add_wave "-noupdate -hex -label CTRL_SRC_RDY_N" $COMP/CTRL_SRC_RDY_N
   add_wave "-noupdate -hex -label CTRL_SOP_N" $COMP/CTRL_SOP_N
   add_wave "-noupdate -hex -label CTRL_EOP_N" $COMP/CTRL_EOP_N
   add_wave "-noupdate -hex -label CTRL_DST_RDY_N" $COMP/CTRL_DST_RDY_N
   add_wave "-noupdate -label CTRL_REQ -hex" $COMP/CTRL_REQ
   add_wave "-noupdate -label CTRL_SEND -hex" $COMP/CTRL_SEND
   add_wave "-noupdate -label CTRL_RELEASE -hex" $COMP/CTRL_RELEASE
   add_wave "-noupdate -label CTRL_RDY -hex" $COMP/CTRL_RDY

   add wave -divider "Statistic data"
   add_wave "-noupdate -label STAT -dec" $COMP/STAT
   add_wave "-noupdate -label STAT_VLD -hex" $COMP/STAT_VLD

   add wave -divider "IBUF status signals"
   add_wave "-noupdate -hex -label LINK_UP          " $COMP/LINK_UP
   add_wave "-noupdate -hex -label INCOMING_PACKET  " $COMP/INCOMING_PCKT

   add wave -divider "Output FrameLink inteface"
   add_wave "-noupdate -hex -label FL_CLK" $COMP/FL_CLK
   add_wave "-noupdate -hex -label TX_SOF_N" $COMP/TX_SOF_N
   add_wave "-noupdate -hex -label TX_SOP_N" $COMP/TX_SOP_N
   add_wave "-noupdate -hex -label TX_EOP_N" $COMP/TX_EOP_N
   add_wave "-noupdate -hex -label TX_EOF_N" $COMP/TX_EOF_N
   add_wave "-noupdate -hex -label TX_SRC_RDY_N" $COMP/TX_SRC_RDY_N
   add_wave "-noupdate -hex -label TX_DST_RDY_N" $COMP/TX_DST_RDY_N
   add_wave "-noupdate -hex -label TX_DATA" $COMP/TX_DATA
   add_wave "-noupdate -hex -label TX_REM" $COMP/TX_REM

   add wave -divider "MI32 interface"
   add_wave "-noupdate -hex -label MI_CLK " $COMP/MI_CLK
   add_wave "-noupdate -hex -label MI     " $COMP/MI
   add_wave "-noupdate -hex -label MI_DWR " $COMP/MI_DWR 
   add_wave "-noupdate -hex -label MI_ADDR" $COMP/MI_ADDR
   add_wave "-noupdate -hex -label MI_RD  " $COMP/MI_RD  
   add_wave "-noupdate -hex -label MI_WR  " $COMP/MI_WR  
   add_wave "-noupdate -hex -label MI_BE  " $COMP/MI_BE  
   add_wave "-noupdate -hex -label MI_DRD " $COMP/MI_DRD 
   add_wave "-noupdate -hex -label MI_ARDY" $COMP/MI_ARDY
   add_wave "-noupdate -hex -label MI_DRDY" $COMP/MI_DRDY

   add wave -divider "Align input"
   add_wave "-noupdate -hex -label rxd_align" $COMP/rxd_align
   add_wave "-noupdate -hex -label rxc_align" $COMP/rxc_align

   add wave -divider "Link up/down logic"
   add_wave "-noupdate -hex -label reg_linkup" $COMP/reg_linkup
   add_wave "-noupdate -hex -label reg_incoming_pckt" $COMP/reg_incoming_pckt
   add_wave "-noupdate -hex -label input_error_vec" $COMP/input_error_vec
   add_wave "-noupdate -hex -label input_error" $COMP/input_error
   add_wave "-noupdate -hex -label seq_link_down" $COMP/seq_link_down
   add_wave "-noupdate -hex -label set_link_down" $COMP/set_link_down
   add_wave "-noupdate -hex -label cnt_error" $COMP/cnt_error
}



