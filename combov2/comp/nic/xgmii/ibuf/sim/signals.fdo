proc XGMII_IBUF_CHECK { COMP } {
   add wave -divider ""
   add wave -divider "CHECK"
   add_wave "-noupdate -label CLK -hex" $COMP/CHECKi/CLK
   add_wave "-noupdate -label RESET -hex" $COMP/CHECKi/RESET

   add wave -divider "Input"
   add_wave "-noupdate -label DATA_RX -hex" $COMP/CHECKi/DATA_RX
   add_wave "-noupdate -label SOP_RX -hex" $COMP/CHECKi/SOP_RX
   add_wave "-noupdate -label EOP_RX -hex" $COMP/CHECKi/EOP_RX
   add_wave "-noupdate -label EOP_POS_RX -hex" $COMP/CHECKi/EOP_POS_RX
   add_wave "-noupdate -label ERR_RX -hex" $COMP/CHECKi/ERR_RX

   add wave -divider "Output"
   add_wave "-noupdate -label DATA_TX -hex" $COMP/CHECKi/DATA_TX
