proc XGMII_IBUF_ALIGN { COMP } {
   add wave -divider ""
   add wave -divider "ALIGN"
   add_wave "-noupdate -hex -label CLK" $COMP/ALIGNi/CLK
   add_wave "-noupdate -hex -label RESET" $COMP/ALIGNi/RESET

   add wave -divider "Input"
   add_wave "-noupdate -hex -label RXD" $COMP/ALIGNi/RXD
   add_wave "-noupdate -bin -label RXC" $COMP/ALIGNi/RXC

   add wave -divider "Output"
   add_wave "-noupdate -hex -label RXD_ALIGNED" $COMP/ALIGNi/RXD_ALIGNED
   add_wave "-noupdate -bin -label RXC_ALIGNED" $COMP/ALIGNi/RXC_ALIGNED
   add_wave "-noupdate -hex -label SOP_ALIGNED" $COMP/ALIGNi/SOP_ALIGNED

   add wave -divider "ALIGN PIPELINE"
	 add wave -divider "1st and 2nd pipeline stage"
	 add_wave "-noupdate -label mask_sop -hex" $COMP/ALIGNi/mask_sop
	 add_wave "-noupdate -label sop_active1 -hex" $COMP/ALIGNi/sop_active1
	 add_wave "-noupdate -label sop_aligned2 -hex" $COMP/ALIGNi/sop_aligned2
	 add_wave "-noupdate -label sop_active2 -hex" $COMP/ALIGNi/sop_active2
	 add_wave "-noupdate -label reg2_sop_aligned -hex" $COMP/ALIGNi/reg2_sop_aligned
	 add_wave "-noupdate -label reg2_rxd -hex" $COMP/ALIGNi/reg2_rxd
	 add_wave "-noupdate -label reg2_rxc -bin" $COMP/ALIGNi/reg2_rxc
	 add_wave "-noupdate -label reg2_sop_align -hex" $COMP/ALIGNi/reg2_sop_align

	 add wave -divider "3rd pipeline stage"
	 add_wave "-noupdate -label reg_rxd_align -hex" $COMP/ALIGNi/reg_rxd_align
	 add_wave "-noupdate -label reg_rxc_align -bin" $COMP/ALIGNi/reg_rxc_align
	 add_wave "-noupdate -label reg_sop_align -hex" $COMP/ALIGNi/reg_sop_align
}


