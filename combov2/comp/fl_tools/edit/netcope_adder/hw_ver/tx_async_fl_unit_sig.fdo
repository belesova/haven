# Paths
set ASYNC_UNIT_PATH           "/testbench/uut"
set TB_BASE                   "/testbench"

source      "signals.fdo"

exec make
view wave
delete wave *

add wave -noupdate -label reset $TB_BASE/reset
add wave -noupdate -label base_clk $TB_BASE/clk

blk_ASYNC_UNIT "$TB_BASE" "$ASYNC_UNIT_PATH"

restart -f
run 10 us
